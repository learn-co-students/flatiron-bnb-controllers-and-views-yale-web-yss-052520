<%= render 'layouts/flash'%>
<%= form_for @review do |f|%>
    <%= f.label :reservation_id%>
    <%= f.collection_select :reservation_id, @reservations, :id, "#{Listing.find(:listing_id).address}"%>
    <%= f.label :description%>
    <%= f.text_area :description%>
    <%= f.label :rating%>
    <%= f.number_field :rating%>
    <%= f.submit %>
<%end%>